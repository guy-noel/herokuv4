{
  "name": "AppSetting",
  "plural": "AppSettings",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "replaceOnPUT": false,
  "mixins": {
    "FireLoop": true,
    "TimeStamp": {
      "required": false,
      "validateUpsert": true
    },
    "Stats": [
      {
        "method": "stats",
        "endpoint": "/stats",
        "description": "Statistical information for AppSetting registers.",
        "type": "model",
        "count": {
          "on": "createdAt",
          "by": "index"
        }
      }
    ]
  },
  "properties": {
    "key": {
      "type": "string",
      "required": true
    },
    "value": {
      "type": "any",
      "required": true
    },
    "type": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "find"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
