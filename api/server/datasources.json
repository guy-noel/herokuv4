{
  "db": {
    "name": "db",
    "connector": "memory"
  },
  "mongodb": {
    "name": "mongodb",
    "connector": "mongodb"
  },
  "freeMobile": {
    "name": "freeMobile",
    "connector": "rest",
    "debug": "false",
    "operations": [
      {
        "template": {
          "method": "GET",
          "url": "https://smsapi.free-mobile.fr/sendmsg",
          "query": {
            "user": "{!user:string}",
            "pass": "{!pass:string}",
            "msg": "{!msg:string}"
          }
        },
        "functions": {
          "sendSMS": [
            "user",
            "pass",
            "msg"
          ]
        }
      }
    ]
  },
  "sigfox": {
    "name": "sigfox",
    "connector": "rest",
    "debug": "true",
    "operations": [
      {
        "template": {
          "method": "GET",
          "url": "https://backend.sigfox.com/api/devices/{!deviceId:string}/messages",
          "headers": {
            "Authorization": "Basic {!credentials:string}"
          },
          "query": {
            "limit": "{!limit=1:number}",
            "before": "{!before:number}"
          }
        },
        "functions": {
          "getBaseStations": [
            "credentials",
            "deviceId",
            "limit",
            "before"
          ],
          "getMessages": [
            "credentials",
            "deviceId",
            "limit",
            "before"
          ]
        }
      },
      {
        "template": {
          "method": "POST",
          "url": "https://backend.sigfox.com/api/devicetypes/{!devicetypeId:string}/callbacks/new",
          "headers": {
            "Authorization": "Basic {!credentials:string}",
            "Content-Type": "application/json"
          },
          "body": "{!body:any}"
        },
        "functions": {
          "createCallbacks": [
            "credentials",
            "devicetypeId",
            "body"
          ]
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "https://backend.sigfox.com/api/devicetypes",
          "headers": {
            "Authorization": "Basic {credentials:string}"
          }
        },
        "functions": {
          "testConnection": [
            "credentials"
          ]
        }
      }
    ]
  },
  "webhook": {
    "name": "webhook",
    "connector": "rest",
    "debug": "true",
    "operations": [
      {
        "template": {
          "method": "{!method:string}",
          "url": "{!url:string}",
          "headers": {
            "Authorization": "Basic {credentials:string}"
          },
          "body": "{json:string}"
        },
        "functions": {
          "send": [
            "url",
            "method",
            "credentials",
            "json"
          ]
        }
      }
    ]
  },
  "here": {
    "name": "here",
    "connector": "rest",
    "debug": "true",
    "operations": [
      {
        "template": {
          "method": "POST",
          "url": "https://pos.cit.api.here.com/positioning/v1/locate?app_id={!app_id:string}&app_code={!app_code:string}",
          "headers": {
            "Content-Type": "application/json"
          },
          "body": "{!json:string}"
        },
        "functions": {
          "locate": [
            "app_id",
            "app_code",
            "json"
          ]
        }
      }
    ]
  },
  "google": {
    "name": "google",
    "connector": "rest",
    "debug": "true",
    "operations": [
      {
        "template": {
          "method": "POST",
          "url": "https://www.googleapis.com/geolocation/v1/geolocate?key={!key:string}",
          "headers": {
            "Content-Type": "application/json"
          },
          "body": "{!json:string}"
        },
        "functions": {
          "locate": [
            "key",
            "json"
          ]
        }
      },
      {
        "template": {
          "method": "GET",
          "url": "https://maps.googleapis.com/maps/api/geocode/json?latlng={!latitude:number},{!longitude:number}&key={!key:string}&language={language:string}"
        },
        "functions": {
          "geocode": [
            "key",
            "latitude",
            "longitude",
            "language"
          ]
        }
      }
    ]
  },
  "ubiscale": {
    "name": "ubiscale",
    "connector": "rest",
    "debug": "true",
    "operations": [
      {
        "template": {
          "method": "POST",
          "url": "https://api.ubignss.com/position",
          "headers": {
            "Content-Type": "application/json",
            "Authorization": "Basic {!credentials:string}"
          },
          "body": "{!json:string}"
        },
        "functions": {
          "locate": [
            "credentials",
            "json"
          ]
        }
      }
    ]
  }
}
