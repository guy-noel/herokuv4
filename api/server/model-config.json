{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "../common/models/alert",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins",
      "../node_modules/loopback3-rt-stats-mixin/dist",
      "../node_modules/@mean-expert/loopback-component-realtime/dist/mixins",
      "../node_modules/loopback-ds-timestamp-mixin"
    ]
  },
  "User": {
    "dataSource": "mongodb",
    "public": false
  },
  "Email": {
    "dataSource": null,
    "public": false
  },
  "AccessToken": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false
        }
      }
    },
    "relations": {
      "user": {
        "type": "belongsTo",
        "model": "user",
        "foreignKey": "userId"
      }
    },
    "acls": [
      {
        "accessType": "*",
        "principalType": "ROLE",
        "principalId": "$owner",
        "permission": "ALLOW",
        "property": "deleteById"
      }
    ]
  },
  "ACL": {
    "dataSource": "mongodb",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongodb",
    "public": false
  },
  "Role": {
    "dataSource": "mongodb",
    "public": true,
    "acls": [
      {
        "accessType": "*",
        "principalType": "ROLE",
        "principalId": "$everyone",
        "permission": "DENY"
      },
      {
        "accessType": "*",
        "principalType": "ROLE",
        "principalId": "admin",
        "permission": "ALLOW",
        "property": "*"
      }
    ]
  },
  "Device": {
    "dataSource": "mongodb",
    "public": true
  },
  "Parser": {
    "dataSource": "mongodb",
    "public": true
  },
  "Category": {
    "dataSource": "mongodb",
    "public": true
  },
  "Message": {
    "dataSource": "mongodb",
    "public": true
  },
  "Geoloc": {
    "dataSource": "mongodb",
    "public": true
  },
  "AppSetting": {
    "dataSource": "mongodb",
    "public": true
  },
  "Dashboard": {
    "dataSource": "mongodb",
    "public": true
  },
  "user": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "emailVerificationRequired": false,
      "allowEternalTokens": true
    }
  },
  "Organization": {
    "dataSource": "mongodb",
    "public": true
  },
  "Alert": {
    "dataSource": "mongodb",
    "public": true
  },
  "AlertValue": {
    "dataSource": null,
    "public": true
  },
  "AlertGeofence": {
    "dataSource": null,
    "public": true
  },
  "AlertHistory": {
    "dataSource": "mongodb",
    "public": true
  },
  "Reception": {
    "dataSource": null,
    "public": true
  },
  "Connector": {
    "dataSource": "mongodb",
    "public": true
  },
  "Widget": {
    "dataSource": "mongodb",
    "public": true
  },
  "Property": {
    "dataSource": null,
    "public": true
  },
  "Beacon": {
    "dataSource": "mongodb",
    "public": true
  },
  "Alexa": {
    "dataSource": null,
    "public": true
  }
}
